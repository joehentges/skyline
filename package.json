{
  "name": "skyline",
  "description": "A Next.js monolithic boilerplate. Quickly get started on your next project.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write src//**/*.{ts,tsx}",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio",
    "db:clear": "tsx ./src/db/clear.ts",
    "db:seed": "tsx ./src/db/seed.ts",
    "db:reset": "npm run db:clear && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@marsidev/react-turnstile": "^1.1.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@react-email/components": "^0.0.39",
    "@t3-oss/env-nextjs": "^0.13.4",
    "argon2": "^0.43.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.39.1",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.511.0",
    "next": "^15.1.6",
    "next-safe-action": "^8.0.4",
    "next-themes": "^0.4.6",
    "pg": "^8.13.0",
    "postgres": "^3.4.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "resend": "^4.5.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.75"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "^22.15.21",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.18.1",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.0.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.5",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
